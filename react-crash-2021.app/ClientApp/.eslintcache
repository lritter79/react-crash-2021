\n                                )}\r\n                            />\r\n                            <Route path='/' exact\r\n                                render={(props) => (\r\n                                    <>\r\n                                        <Header\r\n                                            onAdd={() => setShowAddTask(!showAddTask)}\r\n                                            showAdd={showAddTask} />\r\n                                        <AddTask isToggled={showAddTask} userId={userId} token={token} tasks={tasks} setTasks={setTasks} setShowAddTask={setShowAddTask} />\r\n                                        {!isLoading ? (\r\n                                            (tasks.length > 0) ? (\r\n                                                <Tasks\r\n                                                    tasks={tasks}\r\n                                                    onDelete={onDelete}\r\n                                                    onToggle={toggleReminder}\r\n                                                    onGoToDetail={() => { setShowAddTask(false) }} />) :\r\n                                                ('No Tasks To Show')\r\n                                        ) : ('Loading ...')}\r\n                                    </>\r\n\r\n                                )} />\r\n\r\n                            <Route path='/task/:id' exact\r\n                                render={(props) => (\r\n                                    <TaskDetails\r\n                                        onUpdate={updateTask}\r\n                                        userId={userId}\r\n                                        token={token}\r\n                                    />\r\n                                )}\r\n                            />\r\n                            <Footer isLoggedIn={token} />\r\n                        </>\r\n                    ) : (\r\n                            <>\r\n                                <RegisterAndLoginRoutes setToken={setToken} token={token} />\r\n                            </>\r\n\r\n                        )}\r\n            </div>\r\n            \r\n            \r\n        </Router>\r\n    )\r\n}\r\n\r\nexport default App\r\n\r\n",["152","153"],"C:\\Users\\lritter\\Source\\Repos\\react-crash-2021\\react-crash-2021.app\\ClientApp\\src\\components\\Constant.js",[],["154","155"],"C:\\Users\\lritter\\Source\\Repos\\react-crash-2021\\react-crash-2021.app\\ClientApp\\src\\components\\Footer.js",[],"C:\\Users\\lritter\\Source\\Repos\\react-crash-2021\\react-crash-2021.app\\ClientApp\\src\\components\\comment-components\\Comments.js",["156","157","158"],"C:\\Users\\lritter\\Source\\Repos\\react-crash-2021\\react-crash-2021.app\\ClientApp\\src\\reportWebVitals.js",[],["159","160"],"C:\\Users\\lritter\\Source\\Repos\\react-crash-2021\\react-crash-2021.app\\ClientApp\\src\\components\\task-crud-operations\\FetchTasks.js",["161"],"﻿import Constant from '../Constant'\r\n\r\nconst FetchTasks = async (id, token) => {\r\n    const res = await fetch(Constant() + `/api/Users/${id}/tasks`, {\r\n        method: 'GET',\r\n        headers: {\r\n            'Authorization': 'Bearer ' + token\r\n        }\r\n    })\r\n    const data = await res.json()\r\n\r\n    return data\r\n}\r\n\r\nexport default FetchTasks\r\n","C:\\Users\\lritter\\Source\\Repos\\react-crash-2021\\react-crash-2021.app\\ClientApp\\src\\components\\task-crud-operations\\UpdateTask.js",[],"C:\\Users\\lritter\\Source\\Repos\\react-crash-2021\\react-crash-2021.app\\ClientApp\\src\\components\\task-crud-operations\\FetchTask.js",[],["162","163"],"C:\\Users\\lritter\\Source\\Repos\\react-crash-2021\\react-crash-2021.app\\ClientApp\\src\\components\\comment-components\\CommentSection.js",["164","165","166"],"C:\\Users\\lritter\\Source\\Repos\\react-crash-2021\\react-crash-2021.app\\ClientApp\\src\\components\\task-crud-operations\\CreateTask.js",["167"],"C:\\Users\\lritter\\Source\\Repos\\re